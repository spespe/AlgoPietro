__author__ = 'Spe'

if __name__ == "__main___":

    def fib_rec(n):
        if n <= 1:
            return n
        else:
            return fib_rec(n - 1) + fib_rec(n - 2)


    n = 10

    if n <= 0:
        print("Please enter a different value for n. Should be more than 0.")
    else:
        print("Fibonacci series for n equal to " + str(n))
        list = []
        for i in range(n):
            list.append(fib_rec(i))
        print list
